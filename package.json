  "devDependencies": {
    "@commitlint/cli": "7.6.1",
    "@commitlint/config-conventional": "7.6.0",
    "@commitlint/config-lerna-scopes": "7.6.0",
    "@ndxbn/preset-jest": "file:packages/@ndxbn/preset-jest",
    "@ndxbn/preset-typescript": "file:packages/@ndxbn/preset-typescript",
    "@types/config": "0.0.34",
    "@types/jest": "24.0.13",
    "config": "3.1.0",
    "husky": "^2.3.0",
    "jest": "24.8.0",
    "lerna": "3.14.1",
    "npm-run-all": "4.1.5",
    "prettier": "1.17.1",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "typescript": "3.4.5",
    "winston": "3.2.1"
  },
  "jest": {
    "preset": "@ndxbn/preset-jest",
    "packages": [
      "<rootDir>/packages/*/*"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
